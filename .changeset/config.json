{
  "$schema": "https://unpkg.com/@changesets/config@2.0.0/schema.json",
  "changelog": ["@fuels/changeset", { "repo": "FuelLabs/fuel-bridge" }],
  "commit": false,
  "fixed": [["@fuel-bridge/*"]],
  "ignore": [
    "@fuel-bridge/integration-tests",
    "@fuel-bridge/esbuild-bin-loader",
    "@fuel-bridge/token-deployer"
  ],
  "linked": [],
  "access": "public",
  "baseBranch": "main",
  "updateInternalDependencies": "patch",
  "snapshot": {
    "useCalculatedVersion": true,
    "prereleaseTemplate": "{tag}-{commit}"
  }
}
