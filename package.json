    "prettier": "^2.7.1",
    "prettier-plugin-solidity": "^v1.0.0-rc.1",
    "turbo": "^1.10.7"
  },
  "pnpm": {
    "overrides": {
      "@openzeppelin/upgrades-core": "1.20.5"
    }
  }
