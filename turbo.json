{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "cache": false,
      "outputs": ["dist/**", "typechain/**", "artifacts/**", "out/**"]
    },
    "test": {
      "cache": false
    },
    "check": {
      "dependsOn": ["build"],
      "cache": false
    },
    "format": {
      "cache": false
    }
  }
}
